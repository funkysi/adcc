/* Headings */

h1 {
  font-size:40.0pt;
  padding-top: 35px;
	padding-left:75px; 
}
h2 {
  font-size:20.0pt;
  padding-bottom: 15px;
  padding-top:5px;
}
h3 {
  font-size:16.0pt;
  padding-top:15px;
  
}
strong{
	font-weight:bold;
}
em{
	font-style:italic;
}
.entry-content{
/*	height:250px;*/
}
/* Links */
a {
  color: #000000;
  text-decoration:none;
  font-weight:bold;
}
a:hover{
  text-decoration:underline;
}
a[href^="mailto:"] {
  background: url(../imgs/site/email.png) no-repeat 99% center;
  padding-right: 20px;
}
/* Alignment and formatting classes */
.middle{
	text-align:center;
}
.right{
	text-align:right;
}
.left{
	text-align:left;
}
.bold{
	font-weight:bold;
}
.red{
	color: #f70400;
}
.black{
	color: #000;
}
.padding {
	padding-top:15px;
	padding-bottom:15px;
	padding-right:5px;
}
.big-padding {
	padding-top:30px;
	padding-bottom:30px;
}
.height{height:320px;}

.hidden{
	visibility:hidden;
}
.hide{
	display:none;
	}
.no{

margin-left:190px;
}
/* Class to add dotted lines in sidebar */
.subphoto{
	border-top-width: 1px;
	border-top-style: dashed;
	border-top-color: #eeeeee;
	border-bottom-width: 1px;
	border-bottom-style: dashed;
	border-bottom-color: #eeeeee;
}
/* Images */
img{
	border-top:1px solid #ccc;
	border-left:1px solid #bbb;
	border-right:1px solid #888;
	border-bottom:1px solid #555;	
}
img.clear{
	border:0px;
}

img.afa{
	margin-bottom:15px;
	margin-left:5px;
}
.left {
	float:left;
	margin-right:15px;
	margin-bottom:15px;
}
img.about, img.left{
	float:left;
	margin-right:15px;
	margin-top:15px;
}
img.right{
	float:right;
	margin-left:15px;
	margin-top:15px;
}
img.location{
	margin-top:15px;
	margin-left:60px;


}
hr {
	clear:both;
	border:0px #EfEfFF;
	background:#EfEfFF;
	color:#EfEfFF;
}
hr.clear{
	clear:none;
	}
